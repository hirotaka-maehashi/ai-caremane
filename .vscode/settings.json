{
  "cSpell.enable": false,  // 👈 スペルチェックを無効にする（追加）
  "git.ignoreLimitWarning": true,
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "typescriptreact": "html"
  },
  "cSpell.words": [
  "Soundwave",
  "supabase",
  "Supabase",
  "apiKey",
  "Syxxxxxxxxxxxxxxxx" // 任意の擬似キー
],
"cSpell.ignorePaths": [
  "**/node_modules/**",
  "**/dist/**",
  "src/pages/apikey.tsx",
  "src/pages/index.tsx"
]
}